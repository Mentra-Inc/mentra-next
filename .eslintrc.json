{
    "extends": [
      "next",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error", { "singleQuote": true, "semi": false }],
      "react/react-in-jsx-scope": "off",
      "no-unused-vars": "warn"
    }
  }
  